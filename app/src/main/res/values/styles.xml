<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="SearchView" parent="Widget.AppCompat.SearchView">
        <item name="colorAccent">@color/background</item>
        <item name="android:tint">@color/gray</item>
        <item name="android:editTextColor">@color/gray</item>
        <item name="android:backgroundTint">@color/gray</item>
    </style>
</resources>
